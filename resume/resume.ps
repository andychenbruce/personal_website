/pagewidth  currentpagedevice /PageSize get 0 get def
/pageheight currentpagedevice /PageSize get 1 get def

/indentAmt 25 def
/bulletIndent 30 def
/margin 20 def

% https://www.linuxquestions.org/questions/programming-9/postscript-hyperlinks-574701/
%%
% If pdfmark is defined, use it (for hyperlinks & other pdf-not-postscript features)
% otherwise give pdfmark a null definition so that we don't get any errors.
/pdfmark where {pop} {userdict /pdfmark /cleartomark load put} ifelse

/font { exch findfont exch scalefont setfont } def
/black { 0 0 0 setrgbcolor } def
/title { black	/Times-Roman 24 font drawCenteredText } def
/headerFont { black /AvantGarde-Book 9 font } def
/smallFont { black /Times-Roman 9 font } def

/address {
    gsave
    % 0 0 translate
    smallFont
    pagewidth margin sub -20 alignRight show
    pagewidth margin sub -30 alignRight makeLink
    20 -20 moveto makeLink
    20 -30 moveto makeLink
    20 -40 moveto makeLink
    grestore
} def

/alignRight{
    3 -1 roll
    dup
    getTextWidth
    neg
    4 -1 roll
    add
    3 -1 roll
    moveto
} def

/printInt{
    (%stderr)(w)file
    2 -1 roll
    dup
    3 1 roll
    20 string cvs
    writestring
} def

/hdr {
    0 -15 translate
    20 0 moveto
    1.0 0.5 0.1 setrgbcolor
    /Bookman-Light 16 font
    show
    0 -18 translate
} def

/indent {indentAmt 0 moveto} def

/txt {
    indent
    smallFont
    show
    0 -14 translate
} def

/subHeader {
    exch
    0 -2 translate
    indent
    /Bookman-Demi 10 font
    show
    /Bookman-LightItalic 10 font
    show
    0 -16 translate
} def

/bulletPoint {
    black
    gsave
    smallFont
    bulletIndent 0 translate
    newpath -10 3 1.5 0 360 arc closepath fill
    0 0 moveto
    show
    grestore
    0 -12 translate
} def

/bulletWrap {
    gsave
    smallFont
    bulletIndent 0 translate
    0 0 moveto
    show
    grestore
    0 -12 translate
} def

/getTextWidth{
    stringwidth
    pop
} def

/drawCenteredText{
    dup
    getTextWidth
    2 div
    floor
    neg
    0 rmoveto
    show
} def

/makeLink{
    exch
    currentpoint
    3 2 roll
    dup
    show newpath
    3 1 roll
    moveto
    % https://stackoverflow.com/questions/38391845/postscript-eps-drawing-rectangle-around-text
    [ /Rect
      [ 4 -1 roll
	false charpath pathbbox 
	2 add 4 1 roll 2 add 4 1 roll 2 sub 4 1 roll 2 sub 4 1 roll
	newpath ]
      /Action << /Subtype /URI /URI
		 9 -1 roll
	      >>
      /Subtype
      /Link
      /ANN pdfmark
} def

